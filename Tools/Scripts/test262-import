use strict;
use warnings;
use 5.8.8;

use FindBin;
use Config;
use Encode;

BEGIN {
    $ENV{DBIC_OVERWRITE_HELPER_METHODS_OK} = 1;
    $ENV{T262_EXEC_BIN} = "$FindBin::Bin/test262";

    unshift @INC, "$FindBin::Bin/test262";

    $ENV{LOAD_ROUTES} = 1;
}

use Import;

exit 0;
