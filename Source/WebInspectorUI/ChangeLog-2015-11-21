        (WebInspector.NavigationSidebarPanel.prototype.shouldFilterPopulate):
        (WebInspector.NavigationSidebarPanel.prototype._updateFilter):
        (WebInspector.NavigationSidebarPanel.prototype._treeElementAddedOrChanged):
