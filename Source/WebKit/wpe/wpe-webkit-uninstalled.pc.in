prefix=@CMAKE_BINARY_DIR@
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/DerivedSources

Name: WPE WebKit
Description: Embeddable Web content engine
URL: https://wpewebkit.org
Version: @PROJECT_VERSION@
Requires: glib-2.0 libsoup-2.4 wpe-1.0
Libs: -L${libdir} -lWPEWebKit-@WPE_API_VERSION@
Cflags: -I${includedir}/ForwardingHeaders/wpe -I${includedir}/WebKit/ -I${includedir}/ForwardingHeaders/wpe-jsc -I${includedir}/JavaScriptCore/javascriptcorewpe/
