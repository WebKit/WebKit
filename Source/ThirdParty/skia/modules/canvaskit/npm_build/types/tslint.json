{
    "extends": "dtslint/dtslint.json",
    "rules": {
        "no-bad-reference": true,
        "no-declare-current-package": true,
        "no-self-import": true,
        "no-outside-dependencies": true,

        "no-redundant-jsdoc": false,
        "no-redundant-jsdoc-2": true,

        "npm-naming": [true, { "mode": "code" }]
    }
}
