# The BUILD.gn to build the LLVM and Clang libs that clspv depends on.

import("//build_overrides/clspv.gni")
import("../write_vcsrevision.gni")

config("clspv_llvm_private_config") {
  cflags = [
    "-DBLAKE3_NO_AVX512",
    "-DBLAKE3_NO_AVX2",
    "-DBLAKE3_NO_SSE41",
    "-DBLAKE3_NO_SSE2",
    "-DBLAKE3_USE_NEON=0",
  ]
  include_dirs = [
    "$target_gen_dir/../../../",
    "$root_gen_dir/$clspv_llvm_dir",
    "$clspv_llvm_dir/llvm/include",
  ]

  libs = []
  if (llvm_enable_libxml2) {
    libs += [ "xml2" ]
  }
  if (llvm_enable_zlib) {
    libs += [ "z" ]
  }
  if (llvm_enable_threads) {
    libs += [ "pthread" ]
  }
}

template("clspv_llvm_source_set") {
  source_set(target_name) {
    configs += [ ":clspv_llvm_private_config" ]
    configs += clspv_configs_to_append
    configs -= clspv_configs_to_remove

    # TODO we might not need this
    forward_variables_from(invoker, "*", [ "configs" ])
    if (defined(invoker.configs)) {
      configs += invoker.configs
    }
  }
}

template("clspv_llvm_static_lib") {
  static_library(target_name) {
    configs += [
      ":clspv_llvm_private_config",
      "../../build:llvm_code",
    ]

    configs += clspv_configs_to_append
    configs -= clspv_configs_to_remove

    # add the config header deps here as most need them
    deps = [
      "../config:abi-breaking",
      "../config:config",
      "../config:llvm-config",
    ]

    forward_variables_from(invoker,
                           "*",
                           [
                             "configs",
                             "deps",
                           ])
    if (defined(invoker.configs)) {
      configs += invoker.configs
    }
    if (defined(invoker.deps)) {
      deps += invoker.deps
    }
  }
}

template("clspv_clang_static_lib") {
  static_library(target_name) {
    configs += [
      ":clspv_llvm_private_config",
      "../../build:clang_code",
      "../../build:llvm_code",
    ]

    configs += clspv_configs_to_append
    configs -= clspv_configs_to_remove

    # add the config header deps here as most need them
    deps = []

    forward_variables_from(invoker,
                           "*",
                           [
                             "configs",
                             "deps",
                           ])
    if (defined(invoker.configs)) {
      configs += invoker.configs
    }
    if (defined(invoker.deps)) {
      deps += invoker.deps
    }
  }
}

