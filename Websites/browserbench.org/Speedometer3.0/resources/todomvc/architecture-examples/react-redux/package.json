{
    "name": "todomvc-react-redux",
    "version": "1.0.0",
    "description": "TodoMVC app written with React-Redux.",
    "private": true,
    "engines": {
        "node": ">=18.13.0",
        "npm": ">=8.19.3"
    },
    "scripts": {
        "sanitize": "OUTPUT_FOLDER=./dist node node_modules/sanitize-language",
